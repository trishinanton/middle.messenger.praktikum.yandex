{
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-alert": "off",
    "no-useless-escape": "off",
    "max-len": "off",
    "array-callback-return": "off",
    "no-underscore-dangle": "off",
    "@typescript-eslint/ban-ts-comment": "off"
  }
}
